{
  "name": "@hgraph.io/nextjs-template",
  "version": "0.1.1",
  "private": false,
  "description": "Next.js 16 production-ready template with Hedera integration, TypeScript",
  "bin": "./bin/create-app.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hgraph-io/nextjs-template.git"
  },
  "author": "Hgraph <support@hgraph.com>",
  "keywords": [
    "hedera",
    "walletconnect",
    "reown",
    "hashgraph",
    "hbar",
    "evm",
    "smart contracts"
  ],
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --max-warnings=0",
    "lint:fix": "eslint . --fix",
    "prettier": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "prettier:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "check": "npm run lint && npm run format:check"
  },
  "dependencies": {
    "@hgraph.io/sdk": "0.8.7",
    "autoprefixer": "10.4.19",
    "ethers": "^5.8.0",
    "next": "^16.0.0",
    "postcss": "8.4.31",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.3",
    "@types/node": "20.2.5",
    "@types/react": "19.0.0",
    "@types/react-dom": "19.0.0",
    "eslint": "^9.17.0",
    "eslint-config-next": "16.0.10",
    "prettier": "^3.4.2",
    "typescript": "5.2.2"
  },
  "files": [
    "bin/",
    "src/",
    "public/",
    "README.md",
    "package.json",
    "tsconfig.json",
    "next.config.js",
    "postcss.config.js",
    "next-env.d.ts",
    ".eslintrc.json",
    ".prettierrc"
  ]
}
